generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id   Int    @id @default(autoincrement())
  name String
}

model News {
  id          Int      @id @default(autoincrement())
  text        String
  title       String
  url         String
  publishedAt DateTime
}
