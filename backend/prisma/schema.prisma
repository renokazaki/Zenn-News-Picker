// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  // uncomment next line if you use Prisma <5.10
  // directUrl = env("DATABASE_URL_UNPOOLED")
}

model Task {
  id   Int    @id @default(autoincrement())
  name String
}

model News {
  id    Int    @id @default(autoincrement())
  title String
  text  String
  url   String
}
